<p> New to_do</p>
<%= form_for :todo, url: todos_path do |f| %>
<!-- '/todos' == todos_path -->
	<p>
		<%= f.label :title %><br />
		<%= f.text_field :title %>
	</p>
	<p>
		<%= f.label :datedo %><br />
		<%= f.text_field :datedo %>
	</p>
	<p>
		<%= f.label :text %><br />
		<%= f.text_area :text %>
	</p>

	<p>
		<%= f.submit %>
	</p>
<% end %>